{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "semi": "warn",
      "no-trailing-spaces": "error",
      "indent": ["error", 2],
      "no-unused-vars": "warn"
    },
    "ignorePatterns": [
      "**/*.min.js",
      "**/*.standaline.js",
      "kube.js"
    ]
}