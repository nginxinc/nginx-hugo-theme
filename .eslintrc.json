{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-trailing-spaces": "error",
    "no-unused-vars": "warn",
    "no-undef": "warn"
  },
  "ignorePatterns": ["**/*.min.js", "**/*.standalone.js", "kube.js"]
}
